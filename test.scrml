<html>
<body style="background-color:'#060606'">

${
    component character_card = {
        attributes: {
            charName: "",
            hero: false,
            stars: function(num){
                if (num < 1 || num > 5) return num;
                else return null;
            }
        }
        styles: isolate
            .card {
                height: 200px;
                width: ${cardWidth}px;
            }
        </style>
        <div id=${attributes.charName}>
        const cardWidth = 100;
        if (attributes.hero)<hero_el>
        for (let i = 0 ; i < attributes.stars ; i++){
            <span class="stars">
                <img src="imgs/star.jpg">
            </span>
        }
    }
}

<div id="joey">for (i=0; i < 10 ; i++){
    <p>line # ${i}</p>
}
</div>

</body>
</html>





